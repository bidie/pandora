{
  "name": "@pandorajs/component-dead-counter",
  "version": "2.1.2",
  "description": "Pandora 2 Component Dead Counter",
  "main": "build/src/ComponentDeadCounter.js",
  "types": "build/src/ComponentDeadCounter.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "test": "NODE_ENV=unittest mocha",
    "cov": "nyc mocha",
    "ci": "npm run test"
  },
  "keywords": [
    "pandora"
  ],
  "license": "MIT",
  "devDependencies": {
    "@pandorajs/hub": "^2.1.2",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.0.0",
