    "nyc": "^13.1.0",
    "test-util": "^1.0.0"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "typings",
      "dist"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.d.ts"
  ],
  "dependencies": {
    "@pandorajs/component-decorator": "^2.1.2",
    "@pandorajs/hub": "^2.1.2",
    "debug": "^4.1.1"
