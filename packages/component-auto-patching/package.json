{
  "name": "pandora-component-auto-patching",
  "version": "2.2.0",
  "description": "Pandora 2 module auto patching for trace and console",
  "main": "dist/ComponentAutoPatching",
  "typings": "dist/ComponentAutoPatching.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "start": "npm run build && node dist/index",
    "lint": "../../node_modules/.bin/tslint --format prose -c ../../tslint.json src/**/*.ts test/**/*.ts",
    "test": "npm run lint && NODE_ENV=unittest mocha --trace-warnings",
    "cov": "nyc mocha",
    "ci": "npm run test",
    "gen-api-ref": "../../node_modules/.bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../docs/.vuepress/dist/api-reference/env src/"
  },
  "keywords": [
    "pandora"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/extend": "^3.0.0",
    "@types/lodash": "^4.14.119",
    "@types/mocha": "^2.2.41",
    "@types/node": "^12.0.0",
    "chai": "^4.2.0",
    "egg-logger": "^2.4.1",
    "mm": "^2.1.0",
    "mocha": "^3.5.0",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.4",
    "nock": "^10.0.6",
    "nyc": "^13.1.0",
    "nyc-ts-patch": "^1.0.0",
    "pandora-component-error-log": "^2.2.0",
    "pandora-component-trace": "^2.2.0",
    "pedding": "^1.1.0",
    "sinon": "^7.2.2",
    "urllib": "^2.33.0"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "typings",
      "dist",
      "src/types.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "assert": "^1.4.1",
    "cls-hooked": "^4.2.2",
    "debug": "^4.1.1",
    "is-type-of": "^1.2.1",
    "lodash": "^4.17.11",
    "module-hook": "^2.1.2",
    "opentracing": "^0.14.3",
    "pandora-component-decorator": "^2.1.2",
    "pandora-dollar": "^2.1.2",
    "pandora-tracer": "^2.2.0"
  }
}
